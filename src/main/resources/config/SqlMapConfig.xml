<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 1. typeAliases : VO에 대한 별칭 지정 -->
    <typeAliases>
        <typeAlias alias="Board" type="com.safe.vo.Board"/>
        <typeAlias alias="Food" type="com.safe.vo.Food"/>
        <typeAlias alias="Member" type="com.safe.vo.Member"/>
        <typeAlias alias="Question" type="com.safe.vo.Question"/>
        <typeAlias alias="Reply" type="com.safe.vo.Reply"/>
        <typeAlias alias="Answer" type="com.safe.vo.Answer"/>
        <typeAlias alias="Intake" type="com.safe.vo.Intake"/>
        <typeAlias alias="ExpectedIntake" type="com.safe.vo.ExpectedIntake"/>
    </typeAliases>
	
    <!-- 3. mapper file: table&object mapping -->
	<mappers>
      <mapper resource="config/BoardMapper.xml" />
      <mapper resource="config/FoodMapper.xml" />
      <mapper resource="config/MemberMapper.xml" />
      <mapper resource="config/QnaMapper.xml" />
      <mapper resource="config/IntakeMapper.xml" />
    </mappers>
 
</configuration>



